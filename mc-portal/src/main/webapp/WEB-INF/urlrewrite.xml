<?xml version="1.0" encoding="utf-8"?> 
<!DOCTYPE urlrewrite
    PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN"
    "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>

	<!-- <rule>
		<from>vendor/(.*).css</from>
		<to>assets/vendor/$1.css</to>
	</rule>
	<rule>
		<from>vendor/(.*).js</from>
		<to>assets/vendor/$1.js</to>
	</rule> -->
    <rule>
		<from>static/(.*).css</from>
		<to>assets/css/$1.css</to>
	</rule>
	<rule>
		<from>static/(.*).js</from>
		<to>assets/js/$1.js</to>
	</rule>
	<rule>
		<from>static/(.*).png</from>
		<to>assets/images/$1.png</to>
	</rule>
	<rule>
		<from>static/(.*).jpg</from>
		<to>assets/images/$1.jpg</to>
	</rule>
	<rule>
		<from>static/fonts/(.*).woff2</from>
		<to>assets/css/fonts/$1.woff2</to>
	</rule>	
		
	<rule>
		<from>/$</from>
		<to>/main</to>
	</rule>
	<rule>
	   <name>RULE: ignore js and css and fonts!</name>
	   <from>/^(.*(\.css|\.js|\.png|\.eot|\.ttf|\.svg|\.woff|\.woff2|\.otf))$</from>
	   <to>/main</to>
	</rule> 
	<rule>
		<from>/main/(.*)</from>
		<to>/main?page=$1</to>
	</rule>
	
	
	
	<!-- rule>
		<from>/(.*).html</from>
		<to>/main?page=$1</to>
	</rule>
	<rule>
		<from>/(.*)/</from>
		<to>/main?page=$1</to>
	</rule-->
	<!-- rule>
		<from>/(.*).html</from>
		<to>main.jsp?id=$1</to>
	</rule>
	<rule>
		<from>/main/(.*)/</from>
		<to>/main?name=$1</to>
	</rule-->
</urlrewrite>